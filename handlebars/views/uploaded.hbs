<div class="container mt-5">
    <table class="table table-success table-striped table-hover">
        <thead>
            <tr>
            <th scope="col">Id</th>
            <th scope="col">Producto</th>
            <th scope="col">Precio</th>
            <th scope="col">Imagen</th>
            </tr>
        </thead>
        <tbody>
            <tr>
            <th scope="row">{{data.id}}</th>
            <td>{{data.title}}</td>
            <td>${{data.price}}</td>
            <td><img src="{{data.thumbnail}}" class="img-fluid" alt="..."></td>
            </tr>
        </tbody>
    </table>
</div>
<div class="container mt-5">
    <a class="btn btn-primary" href="/api/productos">Ir a productos</a>
</div>